{
    "default": {
        "name": "Ciphus",
        "version": "1.0.0"
    },
    "server": {
        "errors": {
            "handler": {
                "addres:in:use": false
            }
        },
        "ports": {
            "http": {
                "minimum": 1,
                "maximum": 65535
            }
        },
        "messages": {
            "http": {
                "errors": {
                    "address:access": "The user on this machine does not have enough permissions to open a server.",
                    "address:in:use": "The requested address is already in use by another service."
                }
            }
        }
    }
}